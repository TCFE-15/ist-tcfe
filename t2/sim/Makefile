# ------------------------------------------------------------
# type "make" command in the Linux terminal create report.pdf
#
# type "make clean" to delete all generated files
# ------------------------------------------------------------

all:
	make -f makefile1.mk
	make -f makefile2.mk
	make -f makefile3.mk
	make -f makefile4.mk
	make -f makefile5.mk
clean:
	@rm -f *.log *.tex *.pdf *.ps *~

.PHONY: all clean


